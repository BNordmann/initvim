{
  "YAML header": {
    "prefix": ["yaml", "header"],
    "body": [
        "---",
        "#title: ",
        "subject: \"${1:Subject}\"",
        "author: Birger Nordmann",
        "keywords: [${2:Tags}]",
        "date: $CURRENT_DATE.$CURRENT_MONTH.$CURRENT_YEAR ",
        "toc: false",
        "",
        "---",
        "",
        "${0:Hier k√∂nnte Ihre Werbung stehen.}"
    ],
    "description": "Pandoc compatible YAML header for markdown files."
  },
  "Markdown Image": {
      "prefix": ["![]()"],
      "body": [
        "![${1:caption}](${2:url})${0}"
      ],
      "description": "Include image in markdown"
  },
  "Latex begin/end block": {
    "prefix": ["begin"],
    "body": [
        "\\begin{${1:align}}",
        "    ${0:Math}",
        "\\end{${1}}"
    ],
    "description": "Latex environment"
  }
}
